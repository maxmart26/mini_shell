      SRCS = ${OBJ}
OBJS = ${SRCS:.c=.o}

${LIBFT_PATH}:
				${MAKE} -C ${LIBFT_DIR}

${NAME}: ${SRCS} ${LIBFT_PATH}
	make ${OBJS}
	cc ${CFLAGS} ${OBJS} ${LIBFT_PATH} -o ${NAME} ${LDFLAGS}

clean:
	@$(RM) -r $(OBJ_DIR)
	${MAKE} -C ${LIBFT_DIR} clean
	rm -f ${OBJS}

fclean: clean;
	${MAKE} -C ${LIBFT_DIR} fclean
	rm -f ${NAME}